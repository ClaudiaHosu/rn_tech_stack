{
  "parser": "babel-eslint",

  "extends": [
    "react-native"
  ],

  "ecmaFeatures": {
      "jsx": true
  },

  "rules": {
    react-native/no-color-literals: 0,
    react/prefer-stateless-function: 0,
    class-methods-use-this: 0,
    no-use-before-define: 0,
    import/prefer-default-export: 0,
    no-console: 0,
    arrow-body-style: 0,
    import/no-namespace: 0,
    react/jsx-no-bind: 0
  }
}
